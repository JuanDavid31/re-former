<h1>Create a new user</h1>

<%= form_with model: @user do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit 'Save' %>
<% end %>
